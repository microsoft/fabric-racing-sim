  "queries": [
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "0cdc8d19-882a-46cc-af6a-4371abbe7663"
      },
      "text": "car_telemetry\n| summarize dcount(SessionName)\n",
      "id": "5d8922dd-a71e-4800-bbe7-f881f231dbe8",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "0cdc8d19-882a-46cc-af6a-4371abbe7663"
      },
      "text": "car_telemetry\n| summarize dcount(LapId) by SessionName\n| summarize sum(dcount_LapId)",
      "id": "dec4ee12-1f98-4991-82d8-e49fd6041b8d",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "0cdc8d19-882a-46cc-af6a-4371abbe7663"
      },
      "text": "GetSessionLapData\n| where Rank==1\n| project SessionName",
      "id": "8619401f-7d54-4e8d-907e-60d7701f2456",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "0cdc8d19-882a-46cc-af6a-4371abbe7663"
      },
      "text": "car_telemetry\n| count",
      "id": "e9ddfd6e-1a4d-4576-9644-7649484f39e6",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "0cdc8d19-882a-46cc-af6a-4371abbe7663"
      },
      "text": "let latestSession = car_telemetry | where CarId ==2 |summarize arg_max(StartTime, *) by CarId | project SessionId;\ncar_telemetry\n| where TelemetryPoint in ('Speed')\n| where SessionId in (latestSession)\n| make-series Speed = avg(toreal(Data)*3.6) on StartTime step 1s by LapId\n| order by LapId asc\n| render timechart",
      "id": "6af519d1-a34d-4726-93b4-681bcdd3b336",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "0cdc8d19-882a-46cc-af6a-4371abbe7663"
      },
      "text": "let latestSession = car_telemetry | where CarId ==1 |summarize arg_max(StartTime, *) by CarId | project SessionId;\ncar_telemetry\n| where TelemetryPoint in ('Speed')\n| where SessionId in (latestSession)\n| make-series Speed = avg(toreal(Data)*3.6) on StartTime step 1s by LapId\n| order by LapId asc\n| render timechart",
      "id": "4abf5924-3982-43cf-83da-d40536b4fd8c",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "0cdc8d19-882a-46cc-af6a-4371abbe7663"
      },
      "text": "car_telemetry\n| extend ingesttime = ingestion_time()\n| where ingesttime >= ago(15m)\n| where TelemetryPoint == \"Speed\"\n| extend latency = datetime_diff('millisecond',ingesttime, StartTime)\n| take 1000\n//| summarize displaylatency = percentile(latency, 10)\n| summarize displaylatency=avg(latency)\n| project strcat(displaylatency,\" ms\")",
      "id": "6939cb01-4608-4ec7-a089-d854be86e26a",
      "usedVariables": []
    },
    {
      "dataSource": {
        "kind": "inline",
        "dataSourceId": "0cdc8d19-882a-46cc-af6a-4371abbe7663"
      },
      "text": "GetSessionLapData\n| where Rank==1\n| project LapTime",
